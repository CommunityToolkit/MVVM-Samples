			 xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
			 xmlns:md="clr-namespace:Indiko.Maui.Controls.Markdown;assembly=Indiko.Maui.Controls.Markdown"
			 x:Class="MvvmSampleMAUI.Controls.InteractiveSample">
	<ContentView.Resources>
		<ControlTemplate x:Key="InteractiveSampleTemplate">
			<Border StrokeShape="RoundRectangle 4,4,4,4"
